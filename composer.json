{
    "name": "rmscms/payment",
    "description": "Modular payment gateway toolkit for RMS platform",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "RMS\\Payment\\": "src/",
            "RMS\\Payment\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RMS\\Payment\\PaymentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

